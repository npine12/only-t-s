.then((res) => {\n                setProducts(res.data)\n            })\n            .catch(err => console.log(err))\n    }, [])\n\n    const handleAddToCart = (product_id) => {\n        const product = cart.find((product) => product.product_id === product_id)\n        console.log(product)\n        if (!product) {\n            axios.post(`/api/cart/${product_id}`)\n                .then((res) => {\n                    dispatch(setCart(res.data))\n                })\n                .catch((err) => {\n                    console.log(err)\n                    if (err.response.status === 511) {\n                        props.history.push('/cart')\n                    }\n                })\n        } else {\n            axios.put(`/api/cart/${product_id}`, { quantity: product.quantity + 1 })\n                .then((res) => {\n                    dispatch(setCart(res.data))\n                })\n                .catch(err => {\n\n                    console.log(err)\n                    if (err.response.status === 511) {\n                        props.history.push('/cart')\n                    }\n                })\n        }\n    }\n    return (\n        <div><h1>T-SHIRTS</h1>\n            <div className=\"products-container\">\n                {products.map((product) => {\n                    return (\n                        <div key={product.product_id}>\n                            <img className=\"product-image\" src={product.product_image} />\n                            <h4>{product.product_name}</h4>\n                            <p>{product.product_description}</p>\n                            <button onClick={() => handleAddToCart(product.product_id)}>Add To Cart</button>\n                        </div>\n                    )\n                })}\n            </div>\n        </div>\n    )\n}\n\nexport default Products","/Users/noe/devmountain/unit-7/only-t's/src/components/Footer/Footer.js",[],"/Users/noe/devmountain/unit-7/only-t's/src/components/AboutUs/AboutU